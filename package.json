{
  "name": "image-compress",
  "version": "1.0.0",
  "description": "Compress images on browser with javascript",
  "main": "dist/image-compress.umd.js",
  "scripts": {
    "lint": "xo",
    "build": "rollup -c && npm run uglify",
    "docs": "documentation readme src/image-compress.js --section=API -q --shallow",
    "uglify": "uglifyjs dist/image-compress.umd.js -cm -o dist/image-compress.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/misabitencourt/js-image-compress.git"
  },
  "keywords": [
    "js",
    "image",
    "compress",
    "resize",
    "jpg",
    "picture",
    "photo"
  ],
  "author": "Misael Braga de Bitencourt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/misabitencourt/js-image-compress/issues"
  },
  "homepage": "https://github.com/misabitencourt/js-image-compress#readme",
  "devDependencies": {
    "documentation": "^4.0.0-beta.19",
    "rollup": "^0.41.6",
    "rollup-plugin-buble": "^0.15.0",
    "uglify": "^0.1.5"
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "globals": [
      "document",
      "Image"
    ]
  }
}
